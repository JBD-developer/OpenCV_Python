    "import sys\n",
    "import cv2\n",
    "\n",
    "print(\"Hello OpenCv\", cv2.__version__)\n",
    "\n",
    "img = cv2.imread(\"..\\\\..\\\\Image\\\\lenna.bmp\")\n",
    "\n",
    "if img is None:\n",
    "    print(\"Image load failed!\")\n",
    "    sys.exit()\n",
    "\n",
    "cv2.namedWindow(\"Image\")\n",
    "cv2.imshow(\"image\", img)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
